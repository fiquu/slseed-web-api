  "scripts": {
    "scripts:db:sync": "ts-node scripts/sb.sync.ts",
    "scripts:ses:templates:upload": "ts-node scripts/ses/templates/upload.ts",
    "scripts:users:create": "ts-node scripts/users/create.ts",
    "scripts:users:password": "ts-node scripts/users/password.ts",
    "scripts:users:token": "ts-node scripts/users/token.ts",
    "pretest": "npm run clean",
    "test": "nyc ts-mocha -r tests/helpers/defaults.ts 'tests/**/*.spec.ts'",
    "postversion": "git push --follow-tags",
    "clean": "rimraf .build/",
    "prestart": "npm run clean",
    "start": "sls offline start --stage local",
    "setup:stack": "slseed-utils --do setup-stack",
    "setup:env:development": "slseed-utils --do setup-env --profile development",
    "setup:env:production": "slseed-utils --do setup-env --profile production",
    "setup:env:staging": "slseed-utils --do setup-env --profile staging",
    "setup:env:local": "slseed-utils --do setup-env --profile local",
    "setup:env": "slseed-utils --do setup-env",
    "setup": "slseed-utils",
    "predeploy": "npm test",
    "deploy": "slseed-utils --do deploy",
    "list": "ntl"
  },
  "ntl": {
    "descriptions": {
      "scripts:db:sync": "Syncs the database indexes",
      "scripts:ses:templates:upload": "Uploads a template to SES",
